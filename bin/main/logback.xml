	  <logger name="org.springframework.ws.transport.context" level ="trace"/>
	  <logger name="org.springframework.ws.transport.http.HttpUrlConnection" level ="trace"/>
	  <logger name="org.springframework.security" level ="trace"/>

</configuration>
