<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<include resource="org/springframework/boot/logging/logback/base.xml"/>	
	<!-- <logger name="org.springframework" level="DEBUG"/> -->
	<logger name="org.springframework.ws.context" level="trace"/>
	
	 <logger name="org.springframework.cache" level="trace"/>
	 <logger name="org.springframework.ws" level="trace"/>
	 <logger name="org.springframework.ws.client" level="trace"/>
	 <logger name="org.springframework.ws.client.MessageTracing" level="trace"/>
	 <logger name="org.springframework.ws.client.MessageTracing.sent" level="trace"/>
